<html lang="en">
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="./style.css"/>
  </head>
  <body>
    <nav>
      <ul>
        <li><a href="/">Index</a></li>
        <li><a href="/new">New</a></li>
      </ul>
    </nav>
    <ul class="messages">
      <% messages.map((message, index)=>{ %>
      <li>
        <%= message.user%>: <%= message.text%> @ <%= message.added%>
        <button><a href="/<%= index%>" >View Message</a></button>
      </li>
      <% })%>
    </ul>
  </body>
</html>
